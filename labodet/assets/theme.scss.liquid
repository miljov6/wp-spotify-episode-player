.collection-grid {
  
  .container {
    padding: 24px 23px;
    
    @media (min-width: 768px) {
      padding-left: 15px;
      padding-right: 15px;
    }
  }
  
  .inner {
    
    .iphone-cases {
      height: 518px;
      
      @media (min-width: 768px) {
      	height: auto;
      }
    }

    .item-1 {
      grid-area: item-1;
    }
    
    .item-2 {
      grid-area: item-2;
    }
    
    .item-3 {
      grid-area: item-3;
    }
    
    .item-4 {
      grid-area: item-4;
    }
    
    .item-5 {
      grid-area: item-5;
    }

    @media (min-width: 992px) {
      display: grid;
      grid-template-columns: calc(30% - 10px / 3) calc(40% - 10px / 3) calc(30% - 10px / 3);
      grid-template-rows: auto;
      column-gap: 5px;
      row-gap: 5px;
      grid-template-areas: 
        "item-2 item-1 item-3"
        "item-4 item-1 item-5";
    }

    @media (min-width: 1200px) {
      grid-template-columns: 350 / 1320 * 100% 610 / 1320 * 100% 350 / 1320 * 100%;
    }
  }
  
  .collection {
    position: relative;
  	background-color: #DEDEDE;
    margin-bottom: 65px;
    height: 375px;
    
    @media (min-width: 768px) {
      height: auto;
    }
    
    @media (min-width: 992px) {
      margin-bottom: 0;
      transition: transform 0.6s ease;
      
      &:hover {
        transform: translate(-5px, -15px);
    	box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
        
        .collection-title {
          opacity: 1; 
        }
      }
    }
    
    .collection-image {
      height: 100%;
      width: 100%;
      object-fit: cover;
    }
    
    .collection-title {
      position: absolute;
      bottom: -20px;
      left: 35px;
      right: 36px;
      background: #000;
      color: #fff;
      padding: 10px 20px;
      text-align: center;
      text-decoration: none;
      font-size: 15px;
      font-weight: 400;
      line-height: 24px;
      margin: 0;
      
      @media (min-width: 992px) {
        line-height: 20px;
        bottom: 0;
        left: 0;
        right: 0;
        opacity: 0;
  		transition: opacity 0.6s ease;
      }
    }
  }
}

