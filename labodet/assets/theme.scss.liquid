body {
  position: relative; 
  text-rendering:optimizeLegibility;
  webkit-font-feature-settings: "kern" 1;
  -webkit-font-smoothing: subpixel-antialiased;
}

/* ------ Sale Popup -------- */
body.sale-popup-open {
  overflow: hidden;
  .section-sale-popup {
    display: block;
    overflow: hidden;

    @media (min-width: 768px) {
      display: flex;
    }
  }
  &[data="United States"]{
    &.ov_v{
      overflow-x: hidden;
      overflow-y: auto;
      #site-overlay{
        z-index: -5!important;
        opacity: 0 !important;
      }
    }
    .section-sale-popup {
      display: none;
      &.opened{
        display: block;
        @media (min-width: 768px) {
          display: flex;
        }
      }
    }
  }
}

.section-sale-popup {
  display: none;
  //z-index: 4;
  position: fixed;
  top: 0;
  height: 100%;
  width: 100%;
  z-index: 99;
  overflow: hidden !important;
  overflow-y: auto !important;

  @media (min-width: 768px) {
    justify-content: center;
    align-items: center;
  }

  .inner {
    background: #ffffff;
    min-height: 100%;

    @media (min-width: 768px) {
      max-width: 375px;
      min-height: 0;
    }

    @media (min-width: 992px) {
      display: flex;
      width: 825px;
      max-width: none;
      height: 387px;
      overflow: hidden;
    }
  }

  .logo{
    max-width: 120px;
  }
  .labo-times {
    width: 19px;
    height: 19px;
    z-index: 12;

    justify-content: center;
    top: 0;
    bottom: 0;
    height: auto;
    margin: 0;
    padding: 0;
    background: transparent;
    &:before,
    &:after {
      position: absolute;
      left: 15px;
      content: ' ';
      height: 20px;
      width: 2px;
      background-color: #333;
    }
    &:before {
      transform: rotate(45deg);
    }
    &:after {
      transform: rotate(-45deg);
    }

    svg {
      vertical-align: top;
      display: none !important;
    }
  }

  .header {
    height: 60px;
    display: flex;
    justify-content: center;
    position: relative;

    @media (min-width: 992px) {
      display: none;
    }

    .labo-times {
      position: absolute;
      right: 23px;
      display: flex;
      align-items: center;
      height: 60px;
    }
  }

  .content {
    position: relative;
    padding-left: 23px;
    padding-right: 23px;
    padding-bottom: 23px;
    width: 100%;

    @media (min-width: 992px) {
      padding-left: 59px;
      padding-right: 59px;
      padding-top: 51px;
    }

    .labo-times {
      display: none;

      @media (min-width: 992px) {
        display: block;
        position: absolute;
        right: 20px;
        top: 20px;
        z-index: 2;
      }
    }
  }

  .images {

    @media (min-width: 992px) {
      display: none;
    }

    .slick-dots {
      position: relative;
      margin-top: 16px;
      margin-bottom: 0;

      li {
        margin: 0 3px 0 0;

        &:last-child {
          margin-right: 0;
        }
      }

      button {
        width: 20px;
        background: rgba(0, 0, 0, 0.3);
      }

      .slick-active button {
        background: #000000;
      }
    }
  }

  .single-image {
    display: none;

    @media (min-width: 992px) {
      display: block;
      flex: 1 0 387px;
    }
  }

  .title {
    font-weight: 700;
    font-size: 20px;
    line-height: 20px;
    margin-bottom: 15px;
    margin-top: 23px;

    @media (min-width: 992px) {
      font-size: 24px;
      font-weight: 500;
      margin-bottom: 36px;
      margin-top: 0;
    }
  }

  .sub-title {
    font-weight: 700;
    font-size: 18px;
    line-height: 20px;
    margin-bottom: 20px;

    @media (min-width: 992px) {
      font-size: 24px;
      font-weight: 500;
      margin-bottom: 2px;
    }
  }

  .description {
    background: #FFFFFF;
    font-size: 16px;
    line-height: 20px;
    margin-bottom: 30px;

    @media (min-width: 992px) {
      margin-bottom: 13px;
      font-size: 14px;
    }
  }

  .discount-code {
    margin-bottom: 41px;
    font-size: 16px;

    @media (min-width: 992px) {
      margin-bottom: 38px;
    }

    .code {
      font-size: 35px;
      line-height: 40px;

      @media (min-width: 992px) {
        &:before {
          content: "\a ";
          white-space: pre;
        }
      }
    }
  }

  .btn-black {
    background: #000;
    color: #fff;
    text-transform: uppercase;
    border-radius: 0;
    font-size: 14px;
    padding: 0;
    line-height: 50px;
    border: solid 1px transparent;
    display: block;
    text-align: center;
    @media(max-width: 767px){
      text-transform: none;
      margin: 0 -10px;
      width: auto;
    }

    &:hover {
      color: #000;
      background: #fff;
      border-color: #000;
    }

    @media (min-width: 992px) {
      line-height: 40px;
    }
  }
}

