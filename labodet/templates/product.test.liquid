<div id="SLIDER" class="labo-product-image-gallery-mobile d-md-none">
  {% for image in product.images %}
  <div class="labo-mobile-slide">
    <img src="{{ image | img_url: '1000x' }}" alt="" class="d-block w-100">
  </div>
  {% endfor %}
</div>

<script>
  $('document').ready(function() {
    $('#SLIDER').not('.slick-initialized').slick({
    	slidesToShow: 1,
      	slidesToScroll: 1, 
      	infinite: false,
      	dots: true, 
      	arrows: false,
      	centerMode: true
    });
  });
</script>