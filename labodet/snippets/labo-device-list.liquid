    <div class="labo-product-device-dropdown mob-block">
      <div class="labo-product-device-list labo-mb-selectbox">
        Device: <strong>{{ current_product_name }}</strong>
		<svg class="pc-arrow" width="10" height="5" viewBox="0 0 10 5" fill="none" xmlns="http://www.w3.org/2000/svg">
			<path d="M5 5L10 0H0L5 5Z" fill="black"/>
		</svg>
	
        <div class="arrow-icon">
        		<span class="left-bar"></span>
        		<span class="right-bar"></span>
      		</div>
      </div>

      <ul id="device_list" class="labo-product-device-variants">
        {% for i in fc_keys %}
          {% assign index = i | plus: 0 %}

          <li{% if product.url == all_products[product_array[index]].url %} class="labo-device-list-active"{% endif %}>
            <a href="{{ all_products[product_array[index]].url }}">{{ device_name_array[index] }}</a>
          </li>
        {% endfor %}
      </ul>

    </div>
    <div class="device-list-labo">
    <div class="labo-swatch-header d-none d-md-block header">
      Device: <span>{{ current_product_name }}</span>
    </div>
    <div class="labo-device-product-list">
      {% for i in fc_keys %}
        {% assign index = i | plus: 0 %}
        <div class="{% if product.url == all_products[product_array[index]].url %}labo-device-list-active{% endif %} labo-device-product-item">
          <a href="{{ all_products[product_array[index]].url }}">{{ device_name_array[index] }}</a>
        </div>
      {% endfor %}  
    </div>
  </div>
  </div>
{% endif %}
