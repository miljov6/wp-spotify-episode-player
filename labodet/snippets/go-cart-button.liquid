<!--cart button-->
<div class="go-cart__trigger js-go-cart-trigger">
{% comment %}
    <svg id="cartEmpty" width="20" height="23" viewBox="0 0 18 21" fill="none" xmlns="http://www.w3.org/2000/svg" class="cartIcon"  stroke="black">
        <rect class="fill-rect" y="4" width="18" height="16" />
        <path d="M12 4C12 2.34315 10.6569 1 9 1C7.34315 1 6 2.34315 6 4"/>
    </svg>


    <svg id="cartEmpty" width="18" height="20" viewBox="0 0 18 20" fill="none" xmlns="http://www.w3.org/2000/svg" class="cartIcon"  stroke="black">
        <path fill-rule="evenodd" clip-rule="evenodd" d="M6.5 4C6.5 2.61929 7.61929 1.5 9 1.5C10.3807 1.5 11.5 2.61929 11.5 4H6.5ZM5.5 4C5.5 2.067 7.067 0.5 9 0.5C10.933 0.5 12.5 2.067 12.5 4H18V20H0V4H5.5ZM1 19V5H17V19H1Z" fill="black"/>
    </svg>
     {% endcomment %}
    {% comment %}<svg id="cartEmpty" class="cartIcon"  width="18px" height="20px" viewBox="0 0 18 20" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
        <g id="LABODET-mobile" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
            <path d="M9,1.5 C7.6745166,1.5 6.58996133,2.53153594 6.50531768,3.83562431 L6.5,4 L5.5,4 C5.5,2.06700338 7.06700338,0.5 9,0.5 C10.8685634,0.5 12.3951264,1.9642776 12.4948211,3.8079648 L12.5,4 L18,4 L18,20 L0,20 L0,4 L11.5,4 C11.5,2.61928813 10.3807119,1.5 9,1.5 Z M17,5 L1,5 L1,19 L17,19 L17,5 Z" id="Combined-Shape" fill="#000000" fill-rule="nonzero"></path>
        </g>
    </svg>{% endcomment %}

    {% comment %}<svg id="cartWithItems" class="CartIconfilled" width="18px" height="20px" viewBox="0 0 18 20" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
        <g id="LABODET-mobile" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
            <path d="M9.28223 1.74658C7.95674 1.74658 6.87219 2.77812 6.78754 4.08221L6.78223 4.24658H5.78223C5.78223 2.31359 7.34923 0.746582 9.28223 0.746582C11.1508 0.746582 12.6774 2.21086 12.777 4.05455L12.7822 4.24658H18.2822V20.2466H0.282227V4.24658H11.7822C11.7822 2.86587 10.6629 1.74658 9.28223 1.74658Z" fill="black"></path>
        </g>
        <text id="cartText" class="js-go-cart-counter fill-text" x="9" y="16" font-size="10px" text-anchor="middle" fill="white" data-cart-count>{{- cart.item_count -}}</text>
    </svg>{% endcomment %}
    <svg id="cart_icon_svg" width="18px" height="20px" viewBox="0 0 18 20">
        <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
            <path id="path1" d="M9,1.5 C7.6745166,1.5 6.58996133,2.53153594 6.50531768,3.83562431 L6.5,4 L5.5,4 C5.5,2.06700338 7.06700338,0.5 9,0.5 C10.8685634,0.5 12.3951264,1.9642776 12.4948211,3.8079648 L12.5,4 L18,4 L18,20 L0,20 L0,4 L11.5,4 C11.5,2.61928813 10.3807119,1.5 9,1.5 Z M17,5 L1,5 L1,19 L17,19 L17,5 Z" fill="#000000" fill-rule="nonzero"></path>
            <path id="path2" d="M9.28223 1.74658C7.95674 1.74658 6.87219 2.77812 6.78754 4.08221L6.78223 4.24658H5.78223C5.78223 2.31359 7.34923 0.746582 9.28223 0.746582C11.1508 0.746582 12.6774 2.21086 12.777 4.05455L12.7822 4.24658H18.2822V20.2466H0.282227V4.24658H11.7822C11.7822 2.86587 10.6629 1.74658 9.28223 1.74658Z" fill="black"></path>
        </g>
        <text id="cartText" class="js-go-cart-counter fill-text" x="9" y="16" font-size="" text-anchor="middle" fill="white" data-cart-count>{{- cart.item_count -}}</text>
    </svg>

    {%- comment -%}
    
    {%- endcomment -%}
    <!--go cart mini cart-->
    <div class="go-cart__mini-cart js-go-cart-mini-cart" style="display: none;">

        <div class="go-cart-mini-cart">

            <!--go cart mini cart content-->
            <div class="go-cart-mini-cart__content js-go-cart-mini-cart-content">
            </div>
            <!--end go cart mini cart content-->

            <!--go cart mini cart subtotal-->
            <div class="go-cart-mini-cart__subtotal">
                <span>Subtotal: </span>
                <span class="go-cart-mini-cart__subtotal-price js-go-cart-mini-cart-subtotal"></span>
            </div>
            <!--end go cart mini cart subtotal-->

            <!--go cart mini cart buttons-->
            <div class="go-cart-mini-cart__footer js-go-cart-mini-cart-footer">

                <a href="/checkout/" class="go-cart__button">
                    Proceed to checkout
                </a>

                <a href="/cart/" class="go-cart__button">
                    View cart
                </a>

            </div>
            <!--end go cart mini cart buttons-->

        </div>

    </div>
    <!--end go cart mini cart-->

</div>
<!--end cart button-->

<style>
    .js-go-cart-trigger .CartIconfilled:hover .fill-rect{
        fill: #ffffff;
        stroke: #000000;
    }
    .js-go-cart-trigger .CartIconfilled:hover .fill-text {
        fill: #fff;
    }
</style>
