                  <div class="labo-filter-checkbox">
                    <input type="checkbox" id="All{{ categorytype | replace: " ", "-" }}" name="categorytype" value="{{ categorytype | replace: " ", "-" }}">
                    <label for="All{{ categorytype | replace: " ", "-" }}">{{ categorytype }}</label>
                  </div>
                  {% endfor %}
                </div>
